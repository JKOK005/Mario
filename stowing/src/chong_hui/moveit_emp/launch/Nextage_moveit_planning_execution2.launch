<launch>
 
  <rosparam command="load" file="$(find nextage_moveit_config)/config/joint_names_nextage.yaml" />

  <include file="$(find nextage_moveit_config)/launch/planning_context.launch" >
    <arg name="load_robot_description" value="true" />
  </include>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          

  <include file="$(find nextage_moveit_config)/launch/move_group.launch">
    <arg name="allow_trajectory_execution" value="true"/>
    <arg name="fake_execution" value="false"/>
    <arg name="info" value="true"/>
    <!--arg name="debug" value="$(arg debug)"/-->
  </include>

  <include file="$(find radoe_rviz_panel_plugin)/launch/moveit_rviz.launch">
    <arg name="config" value="true"/>
  </include>
  
<include file="$(find moveit_emp)/launch/motion_plan.launch" />
<node name="moveJoint" pkg="moveit_emp" type="moveJoint" output="screen"/>
<node name="currentPos" pkg="moveit_emp" type="currentPos" output="screen"/>

<node name="nextage_teleoperate_node" pkg="nextage_teleoperate" type="nextage_teleoperate_node" output="screen"/>

</launch>
